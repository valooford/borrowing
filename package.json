{
  "name": "borrowing",
  "version": "1.0.0-beta.1",
  "description": "A utility library that provides borrowing mechanisms via assertion functions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "default": "./dist/next/index.js"
    }
  },
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "lint": "eslint . --cache --flag unstable_native_nodejs_ts_config",
    "format": "prettier . --write",
    "test": "vitest run",
    "prepack": "node --experimental-strip-types ./scripts/prepack.ts",
    "pack": "pnpm prepack && cd dist && pnpm pack --pack-destination ..",
    "prerelease": "pnpm version prerelease --preid beta",
    "publish:npm": "pnpm prepack && cd dist && pnpm publish --access public --tag next"
  },
  "author": "valooford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valooford/borrowing.git"
  },
  "funding": "https://github.com/valooford/valooford#support",
  "homepage": "https://github.com/valooford/borrowing#readme",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@types/node": "^24.2.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "globby": "^14.1.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "3.2.4"
  },
  "keywords": [
    "borrowing",
    "ownership",
    "assertion",
    "typesafe",
    "static-typing",
    "utility",
    "typeguards",
    "typescript",
    "rust",
    "state",
    "state-machine",
    "narrowing",
    "owner",
    "borrow",
    "narrow",
    "capture",
    "primitive",
    "util",
    "tool",
    "tools",
    "safe",
    "type",
    "types",
    "generic",
    "generics",
    "primitive",
    "assert",
    "asserts",
    "validate",
    "validation",
    "check",
    "checking"
  ]
}
