{
  "name": "borrowing",
  "version": "1.0.0-beta.1",
  "description": "A utility library that provides borrowing mechanisms via assertion functions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "default": "./dist/next/index.js"
    }
  },
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsdown --watch -c config/tsdown.config.ts",
    "build": "tsdown -c config/tsdown.config.ts",
    "build:docs": "node --experimental-strip-types ./config/scripts/documenter.ts",
    "lint": "eslint . --cache --flag unstable_native_nodejs_ts_config",
    "format": "prettier . --write",
    "test": "vitest run -c config/vitest.config.ts",
    "prepack": "node --experimental-strip-types ./config/scripts/prepack.ts",
    "pack": "pnpm prepack && cd dist && pnpm pack --pack-destination ..",
    "prerelease": "pnpm version prerelease --preid beta",
    "publish:npm": "pnpm prepack && cd dist && pnpm publish --access public --tag next",
    "prepare": "husky"
  },
  "author": "valooford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valooford/borrowing.git"
  },
  "funding": "https://github.com/valooford/valooford#support",
  "homepage": "https://github.com/valooford/borrowing#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@microsoft/api-extractor": "7.52.11",
    "@microsoft/api-extractor-model": "7.30.7",
    "@microsoft/tsdoc": "^0.15.1",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.2.0",
    "@types/unist": "^3.0.3",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.3.0",
    "globby": "^14.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "mdast-util-from-markdown": "^2.0.2",
    "prettier": "^3.6.2",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "to-vfile": "^8.0.0",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-is": "^6.0.0",
    "unist-util-parents": "^3.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "vfile": "^6.0.3",
    "vitest": "3.2.4"
  },
  "keywords": [
    "borrowing",
    "ownership",
    "assertion",
    "typesafe",
    "static-typing",
    "utility",
    "typeguards",
    "typescript",
    "rust",
    "state",
    "state-machine",
    "narrowing",
    "owner",
    "borrow",
    "narrow",
    "capture",
    "primitive",
    "util",
    "tool",
    "tools",
    "safe",
    "type",
    "types",
    "generic",
    "generics",
    "primitive",
    "assert",
    "asserts",
    "validate",
    "validation",
    "check",
    "checking"
  ]
}
